    "@sendgrid/mail": "^6.3.1",
    "adm-zip": "^0.4.11",
    "app-module-path": "^2.2.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.7",
    "bitcore-lib": "^0.15.0",
    "blob-stream": "^0.1.3",
    "brain.js": "^1.2.2",
    "compression": "^1.7.2",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "countryjs": "^1.8.0",
    "cross-env": "^5.2.0",
    "dateformat": "^3.0.3",
    "dotenv": "^5.0.1",
    "emailjs": "^2.0.0",
    "express-sessions": "^1.0.6",
    "express-useragent": "^1.0.12",
