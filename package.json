    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.19.1",
    "faker": "^4.1.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "nodemon": "^1.17.2",
    "request": "^2.85.0",
    "request-promise": "^4.2.2"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src",
      "test"
    ],
    "transform": {
      ".js": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "srcLogs/*"
    ],
    "delay": "2500"
  }
}
