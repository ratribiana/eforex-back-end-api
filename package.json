{
  "name": "zwap-eforex",
  "version": "1.0.0",
  "homepage": "",
  "main": "src",
  "author": {
    "name": "Robert Anthony Tribiana"
  },
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "build": "npm run clean && babel -d ./build ./src -s && mkdirp ./build/public/files",
    "clean": "rm -rf build && mkdir build",
    "clearDB": "babel-node src/clearDB.js --exec",
    "e2e-test": "cross-env NODE_ENV=test npm run re-seed && jest test/end-to-end-test",
    "eslint": "eslint src/. test/. --config .eslintrc --fix",
    "re-seed": "npm run clearDB && npm run seed && npm run start:dev",
    "seed": "babel-node src/seed.js --exec",
    "start": "if-env cross-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "babel-watch -L src/index.js --exec babel-node",
    "start:prod": "node ./build/index.js",
    "test": "cross-env NODE_ENV=test node_modules/.bin/jest test/unit-test && npm run e2e-test"
  },
  "dependencies": {
    "12factor-config": "^2.0.0",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.0",
